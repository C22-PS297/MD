<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.jualbukuid.ui.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_camera"
        android:name="com.example.jualbukuid.ui.ui.camera.CameraFragment"
        android:label="@string/camera_nav_title"
        tools:layout="@layout/fragment_camera" />

    <fragment
        android:id="@+id/navigation_account"
        android:name="com.example.jualbukuid.ui.ui.account.AccountFragment"
        android:label="@string/account_nav_title"
        tools:layout="@layout/fragment_account" />
</navigation>